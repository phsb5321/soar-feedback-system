{
  "name": "soar-feedback-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev --turbopack",
    "dev:full": "npm run startup -- && next dev --turbopack",
    "build": "next build",
    "start": "npm run startup:prod && next start",
    "start:basic": "next start",
    "start:prod": "npm run startup:prod && npm run start:basic",
    "startup": "dotenv -e .env.local tsx scripts/startup.ts",
    "startup:prod": "node scripts/startup-prod.mjs",
    "lint": "next lint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "db:test": "dotenv -e .env.local tsx scripts/db-utils.ts test",
    "db:migrate": "npm run startup",
    "db:migrate:ai": "dotenv -e .env.local tsx scripts/migrate-ai-fields.ts",
    "db:seed:only": "dotenv -e .env.local tsx scripts/db-utils.ts seed",
    "db:seed:mega": "dotenv -e .env.local tsx scripts/mega-seed.ts",
    "db:status": "dotenv -e .env.local tsx scripts/db-utils.ts status",
    "db:cleanup": "dotenv -e .env.local tsx scripts/db-utils.ts cleanup",
    "db:reset": "npm run db:cleanup && npm run db:migrate",
    "audio:generate": "dotenv -e .env.local tsx scripts/generate-premium-audio.ts",
    "audio:test": "tsx scripts/test-audio.ts",
    "test:contrast": "tsx scripts/test-contrast.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@types/chroma-js": "^3.1.1",
    "@types/color": "^4.2.0",
    "@types/pg": "^8.15.4",
    "chroma-js": "^3.1.2",
    "color": "^5.0.0",
    "dotenv": "^17.0.1",
    "drizzle-orm": "^0.44.2",
    "groq-sdk": "^0.26.0",
    "next": "15.3.4",
    "openai": "^5.10.2",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
